{
  "name": "safecore",
  "version": "0.0.1",
  "private": false,
  "start": "dist/server.js",
  "engines": {
    "node": "v10.14.x"
  },
  "scripts": {
    "start": "NODE_ENV=production npm run build && node -r tsconfig-paths/register ./dist/server.js",
    "dev": "ts-node-dev --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "build:all": "npm run build && npm run build:client",
    "test": "jest",
    "node-gyp": "node-gyp"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.15.2",
    "@ffmpeg-installer/ffmpeg": "^1.0.13",
    "fluent-ffmpeg": "^2.1.2",
    "immutability-helper": "^3.1.1",
    "install": "^0.13.0",
    "lame": "^1.2.4",
    "mumble": "^0.3.21",
    "node-lame": "^1.3.1",
    "npm": "^7.9.0",
    "speaker": "^0.5.3",
    "wav": "^1.0.2",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/ws": "^7.2.7",
    "jest": "^26.6.3",
    "supertest": "^5.0.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.4",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "^4.0.3"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    }
  }
}
