{
  "name": "safecore",
  "version": "0.0.1",
  "private": true,
  "start": "dist/server.js",
  "scripts": {
    "start": "NODE_ENV=production npm run build && node -r tsconfig-paths/register ./dist/server.js",
    "dev": "ts-node-dev --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist && cp -r ./src/public/. ./dist/public && cp -r ./types/. ./dist/types",
    "build:all": "npm run build && npm run build:client",
    "test": "jest",
    "node-gyp": "node-gyp"
  },
  "dependencies": {
    "express": "^4.15.2",
    "immutability-helper": "^3.1.1",
    "mumble": "^0.3.21",
    "mumble-client": "^1.3.0",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/ws": "^7.2.7",
    "dotenv": "^8.2.0",
    "jest": "^26.5.2",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "0.0.9",
    "typescript": "^4.0.3",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    }
  }
}
